{
    "version" : 2,
    "build" : [
        {
            "src": "server.js",
            "use": "@versel/node",
            "config": {
                "includeFiles" :[
                    "dist/**"
                ]
            }
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "server.js"
        }
    ]
}